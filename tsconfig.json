{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "lib": ["es6"],
    "removeComments": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "resolveJsonModule":true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    
    
  }
}